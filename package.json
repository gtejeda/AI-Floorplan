{
  "name": "ai-floorplan",
  "productName": "MicroVillas Investment Platform",
  "version": "1.0.0",
  "description": "Professional desktop application for analyzing and generating Micro Villas real estate investment projects with AI integration, subdivision optimization, and comprehensive financial analysis",
  "main": ".vite/build/main.js",
  "author": {
    "name": "MicroVillas Team",
    "email": "info@microvillas.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ai-floorplan.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/ai-floorplan/issues"
  },
  "homepage": "https://github.com/yourusername/ai-floorplan#readme",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "build": "node scripts/build-and-package.js",
    "build:windows": "node scripts/build-and-package.js --windows",
    "build:macos": "node scripts/build-and-package.js --macos",
    "build:all": "node scripts/build-and-package.js --all-platforms",
    "lint": "eslint --ext .ts,.tsx src/",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "vitest run",
    "test:e2e": "playwright test"
  },
  "keywords": [
    "electron",
    "microvillas",
    "investment",
    "subdivision",
    "desktop"
  ],
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-dmg": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron-forge/plugin-vite": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "@playwright/test": "^1.57.0",
    "@testing-library/react": "^16.3.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^25.0.6",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "@vitejs/plugin-react": "^5.1.2",
    "electron": "^39.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "fs-extra": "^11.3.3",
    "prettier": "^3.7.4",
    "typescript": "^5.7.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "better-sqlite3": "^12.6.0",
    "dotenv": "^17.2.3",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^8.2.0",
    "fabric": "^7.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.12.0",
    "uuid": "^13.0.0",
    "zod": "^4.3.5"
  }
}
